<div class="contenedor--movies">
    <% result.forEach(function(row) { %>
        <% if (row.movies) { %>
            <div class="contenedor--movies--card">
                <%- include('../components/movieCardComponent.ejs', {
                id: row.movies.id,
                title: row.movies.title,
                overview: row.movies.overview,
                genres: 'hola',
                path: row.movies.poster_path,
                vote_average: row.movies.vote_average,
                user: user
                }) %>
            </div>
        <% } else { %>
            <div class="contenedor--movies--card">
                <%- include('../components/movieCardComponent.ejs', {
                id: row.id,
                title: row.title,
                overview: row.overview,
                genres: 'hola',
                path: row.poster_path,
                vote_average: row.vote_average,
                user: user
                }) %>
            </div>
        <% } %>
    <% }); %>
</div>